{
  "name": "iVuePack",
  "version": "0.0.1",
  "description":
    "Scaffold for Creating Engineering Vue.js Project with Webpack, TypeScript",
  "author": "wxyyxc1992",
  "license": "MIT",
  "homepage": "https://github.com/wxyyxc1992/iVuePack",
  "repository": {
    "type": "git",
    "url": "https://github.com/wxyyxc1992/iVuePack"
  },
  "keywords": [
    "boilerplate",
    "live",
    "hot",
    "reload",
    "vue.js",
    "hmr",
    "edit",
    "webpack",
    "babel",
    "PostCSS(FlexBox Polyfill)"
  ],
  "main": "index.js",
  "scripts": {
    "dev": "npm run start",
    "start": "better-npm-run start:dll && better-npm-run start:dev",
    "build": "npm run clean && better-npm-run build",
    "build:ssr":
      "npm run clean && better-npm-run build:ssr && better-npm-run build:server",
    "clean": "rimraf dist",
    "lint": "eslint src",
    "update": "npm-check -u",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "betterScripts": {
    "start:dll": {
      "command": "webpack  --config ./dev-config/webpack/webpack.dll.config.js",
      "env": {
        "NODE_ENV": "development"
      }
    },
    "start:dev": {
      "command":
        "webpack-dashboard -- node --harmony ./dev-config/server/devServer.js --host 0.0.0.0",
      "env": {
        "NODE_ENV": "development"
      }
    },
    "build": {
      "command": "webpack --config ./dev-config/webpack/webpack.config.js",
      "env": {
        "NODE_ENV": "production"
      }
    },
    "build:ssr": {
      "command": "webpack --config ./dev-config/webpack/webpack.config.js",
      "env": {
        "NODE_ENV": "ssr"
      }
    },
    "build:server": {
      "command": "webpack --config ./dev-config/webpack/webpack.server.config.js",
      "env": {
        "NODE_ENV": "production"
      }
    }
  },
  "jest": {
    "moduleFileExtensions": ["js", "jsx"],
    "moduleDirectories": ["node_modules", "bower_components", "shared"],
    "moduleNameMapper": {
      "\\.(css|less|scss)$": "<rootDir>/__mocks__/styleMock.js",
      "\\.(gif|ttf|eot|svg)$": "<rootDir>/__mocks__/fileMock.js"
    }
  },
  "devDependencies": {
    "@storybook/vue": "^3.2.2",
    "autoprefixer": "^7.1.4",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.0",
    "babel-jest": "^21.0.2",
    "babel-loader": "^7.1.2",
    "babel-plugin-async-to-promises": "^1.0.5",
    "babel-plugin-dynamic-import-node": "^1.1.0",
    "babel-plugin-import": "^1.5.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.5.2",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-stage-0": "^6.24.1",
    "babel-preset-vue-app": "^1.3.0",
    "better-npm-run": "^0.1.0",
    "bundle-loader": "^0.5.5",
    "chai": "^4.1.2",
    "copy-webpack-plugin": "^4.0.1",
    "css-loader": "^0.28.7",
    "cssnano": "^3.10.0",
    "eslint": "^4.7.1",
    "eslint-config-airbnb": "^15.1.0",
    "eslint-config-airbnb-base": "^12.0.0",
    "eslint-loader": "^1.8.0",
    "eslint-plugin-babel": "^4.1.2",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jest": "^21.1.0",
    "eslint-plugin-vue": "^2.1.0",
    "express": "^4.15.4",
    "extract-text-webpack-plugin": "3.0.0-beta.3",
    "file-loader": "0.11.2",
    "fs-extra": "^4.0.2",
    "html-loader": "^0.5.1",
    "html-webpack-plugin": "^2.30.1",
    "husky": "^0.14.3",
    "image-webpack-loader": "^3.4.2",
    "less": "^2.7.2",
    "less-loader": "^4.0.4",
    "lint-staged": "^4.2.1",
    
    "object-assign": "^4.1.1",
    "offline-plugin": "^4.8.3",
    "opn": "^5.1.0",
    "postcss-flexibility": "2.0.0",
    "postcss-loader": "^2.0.6",
    "prepack-webpack-plugin": "^1.1.0",
    "prettier": "^1.7.0",
    "raw-loader": "^0.5.1",
    "rimraf": "^2.6.2",
    "sass-loader": "^6.0.6",
    "stack-source-map": "^1.0.6",
    "style-loader": "0.18.2",
    "url-loader": "0.5.9",
    "vue-loader": "^13.0.5",
    "vue-template-compiler": "^2.4.4",
    "wasm-loader": "^1.1.0",
    "webpack": "^3.6.0",
    "webpack-bundle-analyzer": "^2.9.0",
    "webpack-dashboard": "0.4.0",
    "webpack-dev-server": "^2.8.2"
  },
  "dependencies": {
    "element-ui": "^1.4.4",
    "fluent-fetcher": "^0.3.3",
    "jquery": "^3.2.1",
    "vue": "^2.4.4",
    "vue-class-component": "^5.0.2"
  }
}
